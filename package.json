  "main": "''",
  "scripts": {
    "build": "SET NODE_ENV=production && webpack -p --config webpack.production.config.js --progress --profile --colors",
    "dev": "webpack-dev-server --progress --profile --colors --hot --history-api-fallback",
    "lint": "eslint --ext .js --ext .jsx src",
    "start": "start http://localhost:8080/ & npm run dev",
    "server": "node index.js",
    "test": "jest",
    "heroku-postbuild": "NODE_ENV=production && webpack -p --config ./webpack.production.config.js --progress --profile --colors",
    "predeploy": "npm test",
    "deploy": "npm run build",
    "postdeploy": "git push heroku master && heroku open"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
